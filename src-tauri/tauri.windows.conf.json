{
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"identifier": "com.xiaoqiujun.trycase-app",
	"bundle": {
		"targets": ["nsis"],
		"windows": {
			"certificateThumbprint": null,
			"digestAlgorithm": "sha256",
			"timestampUrl": "",
			"webviewInstallMode": {
				"type": "embedBootstrapper",
				"silent": true
			},
			"nsis": {
				"displayLanguageSelector": true,
				"installerIcon": "icons/icon.ico",
				"languages": ["SimpChinese"]
			}
		}
	},
	"app": {
		"windows": [
			{
				"title": "测试用例管理工具",
				"width": 1200,
				"height": 800,
				"minWidth": 1200,
				"minHeight": 800,
				"resizable": true,
				"center": true,
				"shadow": true
			}
		],
		"security": {
			"csp": null
		}
	}
}
